    "bcrypt": "^5.1.1",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.3",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.3.5",
